pipeline
{ 
	agent any
	
	stages
	{
		stage("Build")
		{
			steps
			{
			echo "Building is successfully done...."
			bat "mvn clean"
			}
		}
		stage("Test")
		{
			steps
			{
			echo "Testing is successfully done...."
			bat "mvn test"
			}
		}
		stage("Compile")
		{
			steps
			{
			echo "Compiling is successfully done...."
			bat "mvn compile"
			}
		}
		stage("Deploy")
		{
			steps
			{
			echo "Deploying is successfully done...."		
			}
		}
		
	}
}